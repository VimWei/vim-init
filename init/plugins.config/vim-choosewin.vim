" https://github.com/t9md/vim-choosewin

let g:choosewin_overlay_enable = 1
let g:choosewin_statusline_replace = 0

" nmap - <Plug>(choosewin)
nmap <leader>cw <Plug>(choosewin)

" Tips
" --: Choose the previous window
" -s{label}: swap the window in current tab with current window
" -ss: swaps with the previous window
